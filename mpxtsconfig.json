{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react",
    "baseUrl": "./",
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "types": [
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "build",
    "dist",
    "config"
  ],
  "include": [
    "./src"
  ]
}